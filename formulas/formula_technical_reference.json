  "progression_rules": {
    "description": "Logic for determining weight increases",
    "rules": [
      {
        "cell": "A26",
        "formula": "=IF('WEEK 1 '!F25<1,\"PROCEED TO DOWN SETS\",\"NEW 1 REP MAX ATTEMPT\")",
        "row": 26,
        "type": "max_attempt_trigger"
      },
      {
        "cell": "H26",
        "formula": "=IF('WEEK 1 '!M25<1,\"PROCEED TO DOWN SETS\",\"NEW 1 REP MAX ATTEMPT\")",
        "row": 26,
        "type": "max_attempt_trigger"
      },
      {
        "cell": "O26",
        "formula": "=IF(OR(ISBLANK('WEEK 1 '!T25),ISBLANK('WEEK 1 '!T24),ISBLANK('WEEK 1 '!T23)),\"PROCEED TO DOWNSETS\",if('WEEK 1 '!T25>=6,\"NEW 4 REP MAX ATTEMPT\",\"PROCEED TO DOWN SETS\"))",
        "row": 26,
        "type": "max_attempt_trigger"
      },
      {
        "cell": "B27",
        "formula": "=IF('WEEK 1 '!F25<1,\"\",B25+5)",
        "row": 27,
        "type": "conditional_weight_increase"
      },
      {
        "cell": "I27",
        "formula": "=IF('WEEK 1 '!M25<1,\"\",I25+5)",
        "row": 27,
        "type": "conditional_weight_increase"
      },
      {
        "cell": "P27",
        "formula": "=IF(OR(ISBLANK('WEEK 1 '!T25),ISBLANK('WEEK 1 '!T24),ISBLANK('WEEK 1 '!T23)),\"\",IF('WEEK 1 '!T25>=6,'WEEK 1 '!P20+5,\"\"))",
        "row": 27,
        "type": "conditional_weight_increase"
      },
      {
        "cell": "B28",
        "formula": "=IF('WEEK 1 '!F27<1,\"\",B27+5)",
        "row": 28,
        "type": "conditional_weight_increase"
      },
      {
        "cell": "I28",
        "formula": "=IF('WEEK 1 '!M27<1,\"\",I27+5)",
        "row": 28,
        "type": "conditional_weight_increase"
      },
      {
        "cell": "A48",
        "formula": "=IF('WEEK 1 '!F47<3,\"PROCEED TO DOWN SETS\",\"NEW 4 REP MAX ATTEMPT\")",
        "row": 48,
        "type": "max_attempt_trigger"
      },
      {
        "cell": "B49",
        "formula": "=IF('WEEK 1 '!F47<=2,\"\",B47+5)",
        "row": 49,
        "type": "conditional_weight_increase"
      }
    ]
